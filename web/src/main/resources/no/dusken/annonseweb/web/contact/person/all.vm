<!-- the sidebar -->
#parse('no/dusken/annonseweb/web/contact/sidebar.vm')

<!-- content -->
<div id="content">
    <div class="post">
        <h2 class="title"><a href="/annonse/contact/person/all">KontaktPersonListe</a></h2>
        <div class="entry">
            <ul>
                <!-- should be boxed as sale and invoice to display the most relevant information. -->
                #foreach($contact in $!contactPersonList)
                    <li>
                        <a href="/annonse/contact/person/$contact.Id">$contact.Id - $contact.PersonName</a>
                        -
                        <a href="/annonse/customer/$contact.customer.id">$contact.customer.name</a>
                    </li>
                #end
            </ul>
        </div>
    </div>
</div>


