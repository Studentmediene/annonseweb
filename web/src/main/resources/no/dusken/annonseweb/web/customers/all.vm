<!-- the sidebar -->
#parse('no/dusken/annonseweb/web/customers/sidebar.vm')

<!-- content -->
<div id="content" >
    <div class="post">
        <h2 class="title">
            <a href="/annonse/customers/all">Customer List</a>
        </h2>
        <div class="entry">
            #foreach($customer in $customerList)
                <p>
                    <a href="/annonse/customers/customer/$customer.Id">
                        $customer.Id - $customer.customerName
                    </a>
                </p>
            #end
        </div>
    </div>
</div>
