<!-- the sidebar -->
#parse('no/dusken/annonseweb/web/customer/sidebar.vm')

<!-- content -->
<div id="content" >
    <div class="post">
        <h2 class="title">
            <a href="/annonse/customer/all">Kundeliste</a>
        </h2>
        <div class="entry">
            #foreach($customer in $customerList)
                <p>
                    <a href="/annonse/customer/customer/$customer.Id">
                        $customer.Id - $customer.name
                    </a>
                </p>
            #end
        </div>
    </div>
</div>
